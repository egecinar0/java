
public class CampaignManager {
	
	public void add(Campaign campaign) {
		System.out.println(campaign.getCampaignName()+" adlý kullanýcý eklendi");
	}
	public void remove(Gamer campaign) {
		System.out.println(campaign.getCampaignName()+" adlý kullanýcý silindi");
	}
	public void update(Gamer campaign) {
		System.out.println(campaign.getCampaignName()+" adlý kullanýcý güncellendi");
	}
}
